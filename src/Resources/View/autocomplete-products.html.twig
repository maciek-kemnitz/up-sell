{% for product in products %}
    <li class="item chosen" data-id="{{ product.getShoploProductId() }}" data-thumbnail="{{ product.getThumbnail() }}" data-name="{{ product.getName() }}">
        <ul class="table-columns">
            <li class="with-thumb-50 name">
                <ul class="table-columns">
                    <li class="thumb">
                        <span class="thumb-50">
                            <img src="{{ product.getThumbnail() }}">
                        </span>
                    </li>

                    <li class="text">
                        <p class="bottom name">{{ product.getName() }}</p>
                    </li>
                </ul>
            </li>

            <li class="model">
                {{ product.getOriginalPrice() }} z≈Ç
            </li>

            <li class="action">
                <button class="btn-light add">Dodaj</button>
            </li>
        </ul>
    </li>
{% endfor %}